PORT=3001
# API Configuration
REACT_APP_API_URL=http://localhost:3000/api

# OAuth Configuration
# Get these from your OAuth provider consoles

# Google OAuth
# Get from: https://console.cloud.google.com/apis/credentials
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id_here

# Microsoft OAuth (Azure AD)
# Get from: https://portal.azure.com/#blade/Microsoft_AAD_RegisteredApps
REACT_APP_MICROSOFT_CLIENT_ID=your_microsoft_client_id_here

# Facebook OAuth
# Get from: https://developers.facebook.com/apps/
REACT_APP_FACEBOOK_APP_ID=your_facebook_app_id_here

# Redirect URI (should match what you configured in OAuth providers)
REACT_APP_REDIRECT_URI=http://localhost:3001
